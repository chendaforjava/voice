<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.season.voice.mapper.GuideMapper">

    <select id="findByCityCode" resultType="com.season.voice.entity.Guide">
        select * from guide where city_code like  CONCAT('%',#{cityCode},'%')
    </select>

</mapper>
